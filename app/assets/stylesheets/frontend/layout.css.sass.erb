html
  margin: 0
  padding: 0

body
  margin: 0
  padding: 0
  margin-top: 50px
  font-size: 14px
  min-height: 100%

.content
  background: white

.sessions .background-wrapper, .passwords .background-wrapper
  background: none

.sessions div.container, .passwords div.container
  background: none !important

div.content div.container
  background: none repeat scroll 0 0 #FFFFFF
  padding: 20px
  clear: both
    min-height: 100%
  height: auto !important
    height: 100%
    margin: 0 auto 150px

.background-wrapper
  clear: both
  background: url(<%= asset_data_uri 'bg.png' %>) repeat-x #eee

.navbar
  background: #dbdada url(<%= asset_data_uri 'musicplayer/bg.gif' %>) repeat
  .brand
    background: url(<%= asset_data_uri 'layout/logo.png' %>) no-repeat scroll 0px 0px transparent
    color: transparent
    width: 65px
    height: 45px
    margin: 2px 0 0 -10px !important
    padding: 0 !important
  .users_dropdown
    width: 105px

header
  margin-bottom: 10px

div#ribbon
  padding: 0
  margin: 0
  width: 100%
  -webkit-box-shadow: 0 0 -6px rgba(0, 0, 0, 0.3)
  -moz-box-shadow: 0 0 -6px rgba(0, 0, 0, 0.3)
  box-shadow: 0 0 -6px rgba(0, 0, 0, 0.3)

.heading
  border: 0px
  padding: 7px 14px
  margin: 20px 0
  list-style: none
  background-color: #fbfbfb
  background-image: -moz-linear-gradient(top, white, #f5f5f5)
  background-image: -ms-linear-gradient(top, white, #f5f5f5)
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(white), to(#f5f5f5))
  background-image: -webkit-linear-gradient(top, white, #f5f5f5)
  background-image: -o-linear-gradient(top, white, #f5f5f5)
  background-image: linear-gradient(top, white, #f5f5f5)
  background-repeat: repeat-x
  border: 1px solid #ddd
  -webkit-border-radius: 3px
  -moz-border-radius: 3px
  border-radius: 3px
  filter: progid:dximagetransform.microsoft.gradient(startColorstr='#ffffff', endColorstr='#f5f5f5', GradientType=0)
  -webkit-box-shadow: inset 0 1px 0 #ffffff
  -moz-box-shadow: inset 0 1px 0 #ffffff
  box-shadow: inset 0 1px 0 #ffffff
  font-family: Rationale,sans-serif
  font-size: 22px

blockquote
  font-family: Rationale
  padding-left: 50px
  border-left: none
  background: transparent url(<%= asset_data_uri 'quote.gif' %>) no-repeat
  h3
    font-size: 23px
  small
    color: #777
    font-size: 16px
  ul.entry-title
    padding: 0px
    border: 0px
    padding: 7px 14px
    margin: 0 0 18px
    list-style: none
    background-color: #fbfbfb
    background-image: -moz-linear-gradient(top, white, #f5f5f5)
    background-image: -ms-linear-gradient(top, white, #f5f5f5)
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(white), to(#f5f5f5))
    background-image: -webkit-linear-gradient(top, white, #f5f5f5)
    background-image: -o-linear-gradient(top, white, #f5f5f5)
    background-image: linear-gradient(top, white, #f5f5f5)
    background-repeat: repeat-x
    border: 1px solid #ddd
    -webkit-border-radius: 3px
    -moz-border-radius: 3px
    border-radius: 3px
    filter: progid:dximagetransform.microsoft.gradient(startColorstr='#ffffff', endColorstr='#f5f5f5', GradientType=0)
    -webkit-box-shadow: inset 0 1px 0 #ffffff
    -moz-box-shadow: inset 0 1px 0 #ffffff
    box-shadow: inset 0 1px 0 #ffffff
    li
      font-family: Rationale,sans-serif
      font-size: 22px
div
  &.rightsidebar, &.leftsidebar
    border-right: 1px solid #F0F0F0
    margin-bottom: 20px
  &.rightsidebar
    border-right: 0px
    border-left: 1px solid #F0F0F0
    div.span4
      width: 290px
  &.leftsidebar div.span4
    width: 290px
  &.rightsidebar
    div
      &.span4, &.span1, &.span3
        margin-left: 28px
    h3
      line-height: 36px
  &.leftsidebar h3
    line-height: 36px
  &.rightsidebar div.items, &.leftsidebar div.items
    margin-top: 20px
  &.rightsidebar div.last, &.leftsidebar div.last
    margin-bottom: 20px
  &.rightsidebar div.span3, &.leftsidebar div.span3
    margin: 0 5px
#about
  background: none repeat scroll 0 0 #484848
  padding: 20px
  color: #fff
  #ads
    margin: 0 -20px 0 0

#first_time_vistor
  display: none

img.intro-photo
  margin-bottom: 40px

.carousel .description
  height: 75px

#gallery .thumbnail
  margin-bottom: 20px

#push
  height: 150px
  
