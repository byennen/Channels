= simple_form_for [:admin, @feature], :html => {:multipart => true, :class => 'form-horizontal'} do |f|
  = f.input :audio
  = f.input :vault
  = f.input :news
  = f.input :events
  = f.input :giving

  .form-actions
    = f.button :submit, :class => 'btn-primary'
    or
    = link_to 'Go Back', url_for(:back), :class => 'btn'
