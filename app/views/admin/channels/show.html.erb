
<label class="label"><%= t("activerecord.attributes.admin_channel.name", :default => t("activerecord.labels.name", :default => "Name")) %>:</label>
<p><%= @channel.name %></p>

<div class="form-actions">
<%= link_to "Back", admin_channels_path, :class => 'btn'  %>
<%= link_to "Edit", edit_admin_channel_path(@channel), :class => 'btn' %>
<%= link_to "Delete", admin_channel_path(@channel), :method => "delete", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}", :class => 'btn' %>
</div>
