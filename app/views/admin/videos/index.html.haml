%table.table.table-striped
  %thead
    %tr
      %th Image
      %th Title
      %th
      %th
      %th
  %tbody
    - @videos.each do |video|
      %tr
        %td
        %td video.title
        %td
          %a.btn{:href => edit_admin_channel_video_path(@channel, video)}
            %i.icon-pencil
            Edit
          %a.btn.btn-danger{:href => admin_channel_video_path(@channel, video), :'data-method' => :delete, :'data-confirm' => t('web-app-theme.confirm', :default => 'Are you sure?')}
            %i.icon-trash.icon-white
            Destroy

.form-actions
  %a.btn.btn-primary{:href => new_admin_channel_video_path}
    %i.icon-plus-sign.icon-white
    New Photo
  or
  = link_to 'Go Back', url_for(:back), :class => 'btn'

